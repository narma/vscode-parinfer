{
  "name": "vscode-parinfer",
  "displayName": "Parinfer",
  "description": "Parinfer for VS Code",
  "version": "0.6.1",
  "publisher": "shaunlebron",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunlebron/vscode-parinfer"
  },
  "activationEvents": [
    "onLanguage:clojure",
    "onLanguage:lisp",
    "onLanguage:scheme",
    "onLanguage:racket",
    "onLanguage:extempore"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Parinfer",
      "properties": {
        "parinfer.defaultMode": {
          "type": "string",
          "enum": [
            "INDENT_MODE",
            "PAREN_MODE",
            "DISABLED"
          ],
          "default": "INDENT_MODE",
          "description": "The default mode to be used when opening a new file."
        }
      }
    },
    "keybindings": [
      {
        "command": "parinfer.toggleMode",
        "key": "Ctrl+8",
        "mac": "Cmd+8"
      },
      {
        "command": "parinfer.disable",
        "key": "Ctrl+9",
        "mac": "Cmd+9"
      }
    ],
    "commands": [
      {
        "command": "parinfer.toggleMode",
        "title": "Parinfer: Toggle Mode"
      },
      {
        "command": "parinfer.disable",
        "title": "Parinfer: Disable"
      }
    ]
  },
  "scripts": {
    "format": "standard --fix src/*.js"
  },
  "dependencies": {
    "parinfer": "3.12.0"
  },
  "devDependencies": {
    "standard": "11.0.1"
  },
  "license": "MIT"
}
