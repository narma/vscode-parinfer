{
  "name": "vscode-parinfer",
  "displayName": "Parinfer",
  "description": "Parinfer for VS Code",
  "version": "0.6.1",
  "publisher": "shaunlebron",
  "icon": "icon.png",
  "engines": {
    "vscode": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunlebron/vscode-parinfer"
  },
  "activationEvents": [
    "onLanguage:clojure",
    "onLanguage:lisp",
    "onLanguage:scheme",
    "onLanguage:racket",
    "onLanguage:extempore"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Parinfer",
      "properties": {
        "parinfer.defaultMode": {
          "type": "string",
          "enum": [
            "INDENT_MODE",
            "PAREN_MODE",
            "DISABLED"
          ],
          "default": "INDENT_MODE",
          "description": "The default mode to be used when opening a new file."
        }
      }
    },
    "keybindings": [
      {
        "command": "parinfer.toggleMode",
        "key": "Ctrl+8",
        "mac": "Cmd+8"
      },
      {
        "command": "parinfer.disable",
        "key": "Ctrl+9",
        "mac": "Cmd+9"
      }
    ],
    "commands": [
      {
        "command": "parinfer.toggleMode",
        "title": "Parinfer: Toggle Mode"
      },
      {
        "command": "parinfer.disable",
        "title": "Parinfer: Disable"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch -p ./",
    "format": "standard --fix src/*.ts",
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "parinfer": "3.12.0"
  },
  "devDependencies": {
    "@types/node": "10.5.6",
    "standard": "11.0.1",
    "typescript": "2.9.2"
  },
  "license": "MIT"
}
